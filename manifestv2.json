{

  "manifest_version": 2,
  "name": "Tiktok Snappy Scroller",
  "version": "1.0",

  "description": "Improve tiktok scrolling by adding snapping scroll",

  "icons": {
    "64": "icons/scroll.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.tiktok.com/*"],
      "css": ["main.css"]
    }
  ]
}
